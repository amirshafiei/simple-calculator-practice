pragma solidity 0.8.16;

contract simpleCalc {

    function addition(uint a, uint b) public pure returns(uint result)  {
        return (result= a+b);
    }
    
    function subtraction(uint a, uint b) public pure returns(uint result)  {
        require(a>=b , "result in negative");
        result=a-b;
    }

    function multiplication(uint a, uint b) public pure returns(uint result)  {
        return (result = a*b);
    }

    function division(uint a, uint b) public pure returns(uint result)  {
        require(a>=b && a%b == 0 , "result in float");
        return (result=a/b);
    }

    function power(uint a, uint b) public pure returns(uint result)  {
        result = 1;
        for(uint i=0; i<b; i++){
            result*=a;
        }
        return result;
    }

    function modulo(uint a, uint b) public pure returns(uint result)  {
        require(a>=b , "result in float");
        return (result=a%b);
    }
}

